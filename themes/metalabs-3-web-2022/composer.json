{
	"require": {
		"bornfight/wp-helpers-bf": "*"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/bornfight/wp-helpers-bf"
		},
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"extra": {
		"wordpress-install-dir": "../../../",
		"installer-paths": {
			"../../plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"../../themes/{$name}/": [
				"type:wordpress-theme"
			]
		},
		"dotenv": {
			"path": ".",
			"file": "env"
		},
		"env-map": {
			"secret": "SECRET_FOR_ME"
		}
	},
	"autoload": {
		"files": [
		],
		"classmap": [
		],
		"psr-4": {
			"metalabs3Web2022\\": "app"
		}
	}
}